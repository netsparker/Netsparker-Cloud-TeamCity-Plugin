<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>netsparkercloudscan</name>
    <display-name>Netparker Cloud Scan</display-name>
    <version>@Version@</version>
    <description>Allows to launch Netsparker Cloud scans</description>
    <download-url>www.netsparker.com</download-url>
    <email>support@netsparker.com</email>
    <vendor>
      <name>Netsparker Ltd.</name>
      <url>https://www.netsparker.com</url>
      <logo>https://www.netsparker.com/statics/img/netsparker-desktop-icon.svg</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
